    "css-loader": "^5.2.0",
    "cssnano": "^4.1.11",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "globule": "^1.3.4",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.4.0",
    "npm-check-updates": "^12.5.9",
    "postcss-custom-media": "^8.0.0",
    "postcss-loader": "^5.2.0",
    "postcss-sort-media-queries": "^3.7.7",
